---
permalink: /feed.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>elbatey.twop0intfive.xyz</title>
  <subtitle>Notes on Sovereign Infrastructure and Tech</subtitle>
  <atom:link href="https://elbatey.twop0intfive.xyz/feed.xml" rel="self" type="application/atom+xml" xmlns:atom="http://www.w3.org/2005/Atom" />
  <atom:link href="https://elbatey.twop0intfive.xyz/" rel="alternate" type="text/html" xmlns:atom="http://www.w3.org/2005/Atom" />
  <updated>{{ collections.all | getNewestCollectionItemDate | dateToRfc3339 }}</updated>
  <id>https://elbatey.twop0intfive.xyz/</id>
  <author>
    <name>twop0intfive</name>
  </author>
  {%- for post in collections.all | reverse -%}
  {%- if loop.index0 < 10 -%}
  <entry>
    <title>{{ post.data.title | e if post.data.title else "Untitled Note" }}</title>
    <atom:link href="https://elbatey.twop0intfive.xyz{{ post.url }}" rel="alternate" type="text/html" xmlns:atom="http://www.w3.org/2005/Atom" />
    <updated>{{ post.date | dateToRfc3339 }}</updated>
    <id>https://elbatey.twop0intfive.xyz{{ post.url }}</id>
    <content type="html"><![CDATA[{{ post.templateContent | safe }}]]></content>
  </entry>
  {%- endif -%}
  {%- endfor -%}
</feed>
